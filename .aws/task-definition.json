{
    "family": "web-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::233181867717:role/web-execution-task-role",
    "taskRoleArn": "arn:aws:iam::233181867717:role/web-execution-task-role",
    "containerDefinitions": [
      {
        "name": "web-container",
        "image": "web-ecr",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 8080,
            "containerPort": 8080
          }
        ],
        "environment": [{
             "name" : "DB_HOST",
             "value" : "${{ secrets.RDS_ADDRESS }}"
             },
             {
             "name" : "DB_DATABASE",
             "value" : "${{ secrets.RDS_DB }}"
             },
             {
             "name" : "DB_USERNAME",
             "value" : "${{ secrets.RDS_USER }}"
             },
             {
             "name" : "DB_PASSWORD",
             "value" : "${{ secrets.RDS_PWD }}"
             }],
        "cpu": "256",
        "memory": "512"
      }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
      "FARGATE"
    ]
  }