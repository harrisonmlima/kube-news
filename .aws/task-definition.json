{
    "networkMode": "awsvpc",
    "family": "web-task",
    "executionRoleArn": "arn:aws:iam::233181867717:role/web-execution-task-role",
    "taskRoleArn": "arn:aws:iam::233181867717:role/web-execution-task-role",
    "containerDefinitions": [
      {
        "name": "kube-news",
        "image": "web-ecr",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 8080,
            "containerPort": 8080
          }
        ],
        "environment": [{
            "name" : "DB_HOST",
            "value" : "#{secrets.rds_address}"
            },
            {
            "name" : "DB_DATABASE",
            "value" : "${{ secrets.rds_db }}"
            },
            {
            "name" : "DB_USERNAME",
            "value" : "${{ secrets.rds_user }}"
            },
            {
            "name" : "DB_PASSWORD",
            "value" : "${{ secrets.rds_pwd }}"
            }],
        "cpu": "512",
        "memory": "1024"
      }
    ],
    "cpu": "512",
    "memory": "1024",
    "requiresCompatibilities": [
      "FARGATE"
    ]
  }